<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="com.googlecode._4s_web.client.ui.EventNetwork.MyStyle">
		.node text { pointer-events: none; font: 10px sans-serif;}
		#event-graph {overflow:auto;}
	</ui:style>
	<g:LayoutPanel>
		<g:layer left="0" top="0" width="100%" height="6em">
			<g:HTML>
				<h1>Event Graph</h1>
			</g:HTML>
		</g:layer>
		<g:layer left="0" top="6em" width="100%" bottom="0">
			<g:FlowPanel ui:field='drawingPanel'></g:FlowPanel>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 